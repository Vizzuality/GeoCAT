<a href="#/upload-<%= name %>" class="import_file source">
  <i class="checkbox"></i>
  <%= text %>
</a>
<div class="pane upload">
  <div class="idle">
    <div class="uploader">
      <form>
        <input type="file" />
        <span class="browse">BROWSE</span>
      </form>
      <span class="label">Select a file</span>
    </div>
  </div>
  <div class="split">
    Split file on
    <button data-split='scientificname'>Scientific Name</button>
    <button data-split='group'>Group</button>
  </div>
  <div class="loading">
    <p>Uploading...</p>
    <a href="#/cancel" class="cancel">cancel</a>
  </div>
  <div class="success">
    <p>x ocss</p>
    <a href="#/discard" class="retry">discard</a>
    <a href="#/import" class="import">import</a>
  </div>
  <div class="error">
    <p>There was an error :(</p>
    <a href="#/retry" class="retry">retry</a>
  </div>
</div>
